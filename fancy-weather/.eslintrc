{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],   
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ],
    "linebreak-style": 0
  }
}
